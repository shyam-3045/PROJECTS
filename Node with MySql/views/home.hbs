<h3>Student Management System</h3>
<p><a href="/adduser" class ='addbtn'>Add user</a></p>

<table>
  <thead>
    <tr>
      <td>#</td>
      <td>Name</td>
      <td>Age</td>
      <td>City</td>
      <td>Edit</td>
      <td>Delete</td>
    </tr>

  </thead>
  <tbody>
   {{#each rows}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.name}}</td>
      <td>{{this.age}}</td>
      <td>{{this.city}}</td>
      <td><a class="Edit" href="/edituser/{{this.id}}">Edit</a></td>
      <td><a class="Delete" href="/deleteuser/{{this.id}}">Delete</a></td>
    </tr>


   {{/each}}
  </tbody>

</table>